int a;
int b = 1024*1024*1024;
int c;
int d;
int e;
int f;
while(b) {
    b >>= 1;
    c = 1024*1024*512;
    d = 1;
    while(c) {
        c >>= 1;
        d <<= 1;
        e = 1024*1024*512;
        f = 1;
        while(e) {
            e >>= 1;
            f <<= 1;
        }
        exit(f+1);
        exit(d);
    }
    a+=1;
}

exit(d);
exit(a);